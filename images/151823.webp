<script type="text/javascript">
(function($){
        const players = Array.from(document.querySelectorAll('.embeded-audio-player')).map(p => new Plyr(p));
    $('.plyr').hide();
    $('.embeded-audio-container').click(function(e) {
        e.preventDefault();
        var self = $(this);
        var actBtn = self.find('em.fa');
        actBtn.toggleClass(function(){
            if ($(this).is(".fa-play")) {
                self.addClass('playing');
                $(this).removeClass('fa-play');
                return "fa-pause";
            } else {
                self.removeClass('playing');
                $(this).removeClass('fa-pause');
                return "fa-play";
            }
        });
        var plyr = players[$('.embeded-audio-container').index(this)];
        plyr.togglePlay();
        plyr.on('ended', function(e){
            actBtn.removeClass('fa-pause').addClass('fa-play');
            self.removeClass('playing');
        });
    });
    
    $('a.btn-editor-team').on('click', function(e){
        e.preventDefault();

        if($(this).find('i').hasClass('ui-arrow-down')) {
            $(this).find('i').removeClass('ui-arrow-down').addClass('ui-arrow-up');
        } else {
            $(this).find('i').addClass('ui-arrow-down').removeClass('ui-arrow-up');
        }

        $('#editor-team-container').slideToggle();
    })
})(jQuery);
</script>
<script type="text/javascript">
(function($){
    $('#searchFormTrigger, #searchFormTrigger2').on('click', function(e) {
        e.preventDefault();
        $('#nav__search-box').toggle();
        $('input[name="q"]').focus();
    });

    function toggleDropdown (e) {
        const _d = $(e.target).closest('.dropdown'),
            _m = $('.dropdown-menu', _d);
        setTimeout(function(){
            const shouldOpen = e.type !== 'click' && _d.is(':hover');
            _m.toggleClass('show', shouldOpen);
            _d.toggleClass('show', shouldOpen);
            $('[data-toggle="dropdown"]', _d).attr('aria-expanded', shouldOpen);
        }, e.type === 'mouseleave' ? 100 : 0);
    }

    $('body')
        .on('mouseenter mouseleave','.dropdown',toggleDropdown)
        .on('click', '.dropdown-menu a', toggleDropdown);
})(jQuery);
</script>

